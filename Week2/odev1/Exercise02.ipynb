   ],
   "source": [
    "def faktoriyel(sayi):\n",
    "    deger = 1\n",
    "    for i in range(1, sayi + 1):\n",
    "        deger *= i\n",
    "    return deger\n",
    "\n",
    "sayi = int(input(\"Faktoriyelini hesaplamak istediğiniz sayıyı girin: \"))\n",
    "sonuc = faktoriyel(sayi)\n",
    "print(\"{} sayısının faktöriyeli: {}\".format(sayi, sonuc))\n"
   ]
  },
